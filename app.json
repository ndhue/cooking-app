{
  "expo": {
    "name": "cooking-app",
    "slug": "cooking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/kcal.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#79C388"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
